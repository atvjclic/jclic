   <document>
    <style name="targetError" base="target" foreground="0xFF0000" />
    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="default" family="Arial" background="0xFFFFFF" bold="false" size="18" italic="false" tabWidth="12" />
    <section>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="true" background="0xFFFFFF" size="18">JEITO DE SER   </text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">SE VOCÊ FOSSE UM BICHO,</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">QUE TIPO DE BICHO GOSTARIA DE SER?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">BICHO </text>
      <target>BRAVO,</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18"> </text>
      <target>FEROZ</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">,</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">DE GARRAS </text>
      <target>AFIADAS</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">BICHO </text>
      <target>MANSO</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18"> E </text>
      <target>CARINHOSO</target>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">DE PELO </text>
      <target>MACIO</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">UM BICHO </text>
      <target>PREGUIÇOSO</target>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">OU UM BICHO </text>
      <target>ENGRAÇADO</target>
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">UM BICHO QUE VOA</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">LÁ NO CÉU...?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFEFEFE" size="18">UM BICHO QUE CORRE, OU QUE</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFFFFFF" size="18">SE ARRASTA NO CHÃO?</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF" />
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFFFFFF" size="18">BICHO </text>
      <target>ESPINHENTO</target>
      <text family="Arial" bold="false" background="0xFFFFFF" size="18">, OU </text>
      <target>PELUDO</target>
      <text family="Arial" bold="false" background="0xFFFFFF" size="18">...</text>
     </p>
     <p family="Arial" Alignment="1" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFFFFFF" size="18">UM BICHO-PAPÃO?</text>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" Alignment="2" background="0xFFFFFF">
      <text family="Arial" bold="false" background="0xFFFFFF" size="12">RIBEIRO, Nye. São Paulo: Editora do Brasil, 2000. p. 8</text>
     </p>
    </section>
   </document>
